<li id="header_input_li">
  <img id="header_input_submit_button" onclick="header_add_plan();" src="/images/buttons/add_plan.png" />  <div id="header_input_exterior"><input type="text" id="header_input" default_value="search plans, people and categories" tip="enter name of plan, person or category" tabindex="1" css_size="large" /></div>
  
  <script type="text/javascript">
    /**
     * Selects an item from the header input autoselect suggestions
     *
     */
    function header_input_select()
    { 
    	window.location.href = $(this).attr('href'); 
    }
    
    /**
     * Redirects to dedicated search results page with input value as query
     * 
     */
    function header_input_submit(value)
    {
    	window.location.href = routes['search'] + '?q=' + encodeURIComponent(value);
    	
    	return 2; // preserve the input value as page loads
    }
    
    /**
     * Submits the contents of the header input to trigger an Add Plan form prompt
     * 
     */
    function header_add_plan()
    {
      var input = $('#header_input');
    	var value = trim(input.val());

    	if(!session_user_id)
    	{
    		if(value && value != input.attr('default_value'))
    		{
    			var reason = 'add this plan';
    		}
    		else
    		{
    			var reason = 'add a plan';
    		}

    		prompt_toggle(true,'sign_in', { reason: reason, callback: function() { header_add_plan(); } });

    		return false;
    	}

      // Clear value if it's just the default
    	if(value == input.attr('default_value'))
    	{
    		value = '';
    	}

    	prompt_toggle(true, 'plan_add', { what_string: value });

    	track_event('Share plan', 'Header input', value);
    }
  
    // Initialize the header input autosuggest
    var header_input = new InputConfig($('#header_input'), {
      autosuggest: 
      { 
        url: routes['search_universal'], 
        params:
        {
          select_callback: header_input_select,
          submit_callback: header_input_submit
        }
      }
    });
  </script>
</li>
  
          </ul>
        </div>
    
        <div style="clear:both"></div>
    
      </div>
    
    </div>
    
